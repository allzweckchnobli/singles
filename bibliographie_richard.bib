	keywords = {conceptual knowledge acquisition, example-based learning, observational learning, productive failure, vicarious failure},
	file = {Full Text PDF:/Users/richardconrardy/Zotero/storage/83VURVIU/Hartmann et al. - 2022 - Productive versus vicarious failure Do students n.pdf:application/pdf;Snapshot:/Users/richardconrardy/Zotero/storage/NV69XMY4/acp.html:text/html},
