	date = {2019-10-01},
	file = {Full Text:/Users/richardconrardy/Zotero/storage/4VQS7GKS/Findeisen et al. - 2019 - Lernen durch Videos – Empirische Befunde zur Gesta.pdf:application/pdf},
}

