	langid = {english},
	keywords = {Meta-analysis, Primary school, Review, Self-regulated learning, Strategy training},
	file = {ScienceDirect Full Text PDF:/Users/richardconrardy/Zotero/storage/FHWUKZU8/Dignath et al. - 2008 - How can primary school students learn self-regulat.pdf:application/pdf;ScienceDirect Snapshot:/Users/richardconrardy/Zotero/storage/L8H9UT2U/S1747938X08000080.html:text/html},
