@article{kruger_unskilled_2002,
	title = {Unskilled and unaware--but why? A reply to Krueger and Mueller (2002)},
	volume = {82},
	issn = {0022-3514},
	shorttitle = {Unskilled and unaware--but why?},
	abstract = {J. Kruger and D. Dunning (1999) argued that the unskilled suffer a dual burden: Not only do they perform poorly, but their incompetence robs them of the metacognitive ability to realize it. J. Krueger and R. A. Mueller (2002) replicated these basic findings but interpreted them differently. They concluded that a combination of the better-than-average ({BTA}) effect and a regression artifact better explains why the unskilled are unaware. The authors of the present article respectfully disagree with this proposal and suggest that any interpretation of J. Krueger and R. A. Mueller's results is hampered because those authors used unreliable tests and inappropriate measures of relevant mediating variables. Additionally, a regression-{BTA} account cannot explain the experimental data reported in J. Kruger and D. Dunning or a reanalysis following the procedure suggested by J. Krueger and R. A. Mueller.},
	pages = {189--192},
	number = {2},
