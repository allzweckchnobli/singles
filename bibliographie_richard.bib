	langid = {english},
	keywords = {Problem-solving, Productive failure, 4C/{ID}, Deductive teaching, Expository instruction, Inductive teaching, Inquisitory instruction, Problem-based learning},
	file = {Full Text PDF:/Users/richardconrardy/Zotero/storage/QPJTUCNR/Gorbunova et al. - 2023 - Are Inductive Teaching Methods Compatible with Cog.pdf:application/pdf},
